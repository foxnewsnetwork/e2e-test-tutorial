{
  "name": "e2e-test-tutorial",
  "version": "0.0.0",
  "description": "End to end acceptance / spec testing tutorial for ramping up QA",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Thomas Chen",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "git@github.com:foxnewsnetwork/e2e-test-tutorial.git",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "github-pages": "ember github-pages:commit --message='gh page commit' --environment=development",
    "push-gh-pages": "git push origin gh-pages:gh-pages",
    "github": "npm run github-pages && npm run push-gh-pages"
  },
  "dependencies": {
    "ember-cli-babel": "^6.3.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.15.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-github-pages": "^0.1.2",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "^0.3.4",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.15.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-paper": "^1.0.0-beta.3",
    "ember-resolver": "^4.0.0",
    "ember-selenium-build": "^0.2.0",
    "ember-source": "~2.15.0",
    "express": "^4.15.4",
    "glob": "^4.5.3",
    "loader.js": "^4.2.3",
    "morgan": "^1.8.2",
    "showdown": "^1.7.4",
    "showdown-highlightjs-extension": "^0.1.2"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
